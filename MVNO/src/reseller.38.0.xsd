<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.verizonwireless.com/oas" xmlns="http://www.verizonwireless.com/oas" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
	attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation>
		</xs:documentation>
	</xs:annotation>
	<!-- ***BASE STRING TYPES*** -->
	<xs:complexType name="oasStringType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="old" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 1 *** -->
	<xs:complexType name="oasSingleCharacterType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="1"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 10 *** -->
	<xs:complexType name="oasString10Type">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 60 *** -->
	<xs:complexType name="oasString60Type">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="60"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 100 *** -->
	<xs:complexType name="oasString100Type">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 120 *** -->
	<xs:complexType name="oasString120Type">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="120"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 140 *** -->
	<xs:complexType name="oasString160Type">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="160"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<!-- *** 500 *** -->
	<xs:complexType name="oasString500Type">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="500"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="oasDecimalType">
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="old" type="xs:decimal"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!-- ***PERSONAL INFO TYPES (STRINGS)*** -->
	<xs:complexType name="personNameType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="25"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="streetNameType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="70"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="cityType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="35"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="stateType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="[A-Z]{2}"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="countryType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="3"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="zipCodeType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="[1-9][0-9][0-9][0-9][0-9]|[0-9][1-9][0-9][0-9][0-9]|[0-9][0-9][1-9][0-9][0-9]|[0-9][0-9][0-9][1-9][0-9]|[0-9][0-9][0-9][0-9][1-9]"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="phoneType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="\d{10}"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
    <!-- ***ACCOUNT INFO TYPES (STRING)*** -->
	<xs:complexType name="referenceNumberType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="100"/>
				<xs:minLength value="5"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="subReferenceNumberType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="110"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ospAccountNoType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="[A-Z0-9]{0,20}"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="ssnType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
                <!--  <xs:pattern value="[0-9]{3}\-[0-9]{2}\-[0-9]{4}|\d{9}"/>  -->
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="taxIdType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
                <!--  <xs:pattern value="[0-9]{2}\-[0-9]{7}|\d{9}"/>  -->
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="pinType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:maxLength value="15"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="yesNoType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="[YN]"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="addDeleteType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="[AD]{1}"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="includedWithPlanType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="[YNB]{1}"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="mdnSimpleType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="mdnType">
		<xs:simpleContent>
			<xs:extension base="mdnSimpleType">
				<xs:attribute name="type" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="minType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="npaNxxSimpleType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="npaNxxType">
		<xs:simpleContent>
			<xs:extension base="npaNxxSimpleType">
				<xs:attribute name="type" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="esnType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{11}|[A-Fa-f0-9]{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="meidType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[9][0-9]{13}|[3][5-6][0-9]{12}|[A-Fa-f][A-Fa-f0-9]{13}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="esnMeidType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[9][0-9]{13}|[3][5-6][0-9]{12}|[A-Fa-f][A-Fa-f0-9]{13}|[0-9]{11}|[A-Fa-f0-9]{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="globalSIMCardType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{19}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="deviceIdType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[9][0-9]{13}|[3][5-6][0-9]{12}|[A-Fa-f][A-Fa-f0-9]{13}|[0-9]{11}|[A-Fa-f0-9]{8}|[0-9A-F]{16}|[0-9A-F]{15}|[0-9A-F]{14}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="imeiType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{14,16}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SIMCardType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{20}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="reasonCodeType">
		<xs:simpleContent>
			<xs:extension base="oasStringType">
				<xs:attribute name="description" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="competitorCodeType">
		<xs:simpleContent>
			<xs:extension base="oasStringType">
				<xs:attribute name="description" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="oasDateType">
		<xs:simpleContent>
			<xs:restriction base="oasStringType">
				<xs:pattern value="\d{2}\d{2}\d{4}|\d{2}/\d{2}/\d{4}|\d{2}/\d{2}/\d{4} \d{2}:\d{2}:\d{2}"/>
			</xs:restriction>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="desiredDueDateType">
		<xs:annotation>
			<xs:documentation>Format is standard XML dateTime without the time zone offset: YYYY-MM-DDTHH:MI:SS</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime">
			<xs:pattern value="[0-9]{4}\-[0-9]{2}\-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="minWithOldMinType">
		<xs:simpleContent>
			<xs:extension base="minType">
				<xs:attribute name="old" type="minType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="mdnWithOldMdnType">
		<xs:simpleContent>
			<xs:extension base="mdnType">
				<xs:attribute name="old" type="mdnSimpleType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="esnWithOldEsnType">
		<xs:simpleContent>
			<xs:extension base="esnType">
				<xs:attribute name="old" type="esnType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="esnWithOldEsnMeidType">
		<xs:simpleContent>
			<xs:extension base="esnType">
				<xs:attribute name="old" type="esnMeidType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="meidWithOldEsnMeidType">
		<xs:simpleContent>
			<xs:extension base="meidType">
				<xs:attribute name="old" type="esnMeidType" use="required"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="deviceIdWithOldDeviceIdType">
		<xs:simpleContent>
			<xs:extension base="deviceIdType">
				<xs:attribute name="old" type="deviceIdType"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="SIMCardWithOldSIMCardType">
		<xs:simpleContent>
			<xs:extension base="SIMCardType">
				<xs:attribute name="old" type="SIMCardType"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="systemDataType">
		<xs:sequence>
			<xs:element name="marketCode" type="oasStringType" minOccurs="0"/>
			<xs:element name="regionCode" type="oasStringType" minOccurs="0"/>
			<xs:element name="areaName" type="oasStringType" minOccurs="0"/>
			<xs:element name="fulfillment" type="oasStringType" minOccurs="0"/>
			<xs:element name="databaseID" type="oasStringType" minOccurs="0"/>
			<xs:element name="bta" type="oasStringType" minOccurs="0"/>
			<xs:element name="msa" type="oasStringType" minOccurs="0"/>
			<xs:element name="serviceZipCode" type="zipCodeType"/>
			<xs:element name="serviceCity" type="oasStringType" minOccurs="0"/>
			<xs:element name="serviceState" type="stateType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ssnOrTaxIdType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="ssn" type="ssnType"/>
				<xs:element name="taxId" type="taxIdType"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lnpAddressType">
		<xs:sequence>
			<xs:element name="addressLine1" type="streetNameType"/>
			<xs:element name="addressLine2" type="streetNameType" minOccurs="0"/>
			<xs:element name="city" type="cityType"/>
			<xs:element name="state" type="stateType"/>
			<xs:element name="zip" type="zipCodeType"/>
			<xs:element name="country" type="countryType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lnpPersonNameType">
		<xs:sequence>
			<xs:element name="first" type="personNameType"/>
			<xs:element name="middleInitial" type="oasSingleCharacterType" minOccurs="0"/>
			<xs:element name="last" type="personNameType"/>
			<xs:element name="suffix" type="oasString10Type" minOccurs="0"/>
			<xs:element name="prefix" type="oasString10Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lnpNameType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="name" type="lnpPersonNameType"/>
				<xs:element name="business" type="oasString60Type"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="authorizationStatusType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y"/>
			<xs:enumeration value="N"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="directionalIndicatorType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="E"/>
			<xs:enumeration value="W"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="N"/>
			<xs:enumeration value="NE"/>
			<xs:enumeration value="NW"/>
			<xs:enumeration value="SE"/>
			<xs:enumeration value="SW"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="hotCutType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y"/>
			<xs:enumeration value="N"/>
		</xs:restriction>
	</xs:simpleType>
<!-- RESELLER HBQ Qualification Info - release 13.2.0 -->
	<xs:simpleType name="qualificationType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-3]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="lnpType">
		<xs:sequence>
			<xs:element name="lnpName" type="lnpNameType"/>
			<xs:element name="address" type="lnpAddressType"/>
			<xs:element name="authorizedSigner" type="oasString60Type" minOccurs="0"/>
			<xs:element name="authorizationStatus" type="authorizationStatusType"/>
			<xs:element name="directionalIndicator" type="directionalIndicatorType" minOccurs="0"/>
			<xs:element name="phoneUserName" type="oasStringType" minOccurs="0"/>
			<xs:element name="desiredDueDate" type="desiredDueDateType" minOccurs="0"/>
			<xs:choice>
				<xs:sequence>
					<xs:element name="ssnOrTaxId" type="ssnOrTaxIdType"/>
					<xs:element name="ospAccountNo" type="ospAccountNoType" minOccurs="0"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="ospAccountNo" type="ospAccountNoType"/>
				</xs:sequence>
			</xs:choice>
			<xs:element name="hotCut" type="hotCutType" minOccurs="0"/>
			<xs:element name="pin" type="pinType" minOccurs="0"/>
			<xs:element name="remark" type="oasString160Type" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
<!-- RESELLER LNP Enhancement - release 15.4.0 -->
	<xs:complexType name="mdnDetailType">
		<xs:sequence>
			<xs:element name="mdn" type="mdnType"/>
			<xs:element name="reasonCode" type="oasStringType"/>
			<xs:element name="reasonDetail" type="oasStringType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lnpUpdateType">
			<xs:sequence>
				<xs:element name="lnpName" type="lnpNameType"/>
				<xs:element name="address" type="lnpAddressType"/>
				<xs:element name="directionalIndicator" type="directionalIndicatorType" minOccurs="0"/>
				<xs:choice>
					<xs:sequence>
						<xs:element name="ssnOrTaxId" type="ssnOrTaxIdType"/>
						<xs:element name="ospAccountNo" type="ospAccountNoType" minOccurs="0"/>
					</xs:sequence>
					<xs:sequence>
						<xs:element name="ospAccountNo" type="ospAccountNoType"/>
					</xs:sequence>
				</xs:choice>
				<xs:element name="pin" type="pinType" minOccurs="0"/>
			</xs:sequence>
	</xs:complexType>	
	<xs:complexType name="featureType">
		<xs:sequence>
			<xs:element name="featureCode" type="oasStringType"/>
			<xs:element name="subscribe" type="addDeleteType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="featureRPType">
		<xs:sequence>
			<xs:element name="featureCode" type="oasStringType"/>
			<xs:element name="includedWithPlan" type="includedWithPlanType" minOccurs="0"/>
			<xs:element name="subscribe" type="addDeleteType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="planType">
		<xs:sequence>
			<xs:element name="planCode" type="oasStringType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ratePlanType">
		<xs:sequence>
			<xs:element name="plan" type="planType"/>
			<xs:element name="feature" type="featureRPType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="equipmentType">
		<xs:sequence>
			<xs:element name="make" type="oasStringType"/>
			<xs:element name="model" type="oasStringType"/>
			<xs:element name="mode" type="oasStringType"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ***MESSAGE HEADER*** -->
	<xs:complexType name="messageHeaderType">
		<xs:sequence>
			<xs:element name="versionNumber" type="xs:string" minOccurs="0"/>
			<xs:element name="vendorId" type="xs:string"/>
			<xs:element name="channelId" type="xs:string" minOccurs="0"/>
			<xs:element name="channelType" type="xs:string" minOccurs="0"/>
			<xs:element name="requestType" type="xs:string"/>
			<xs:element name="orderType" type="xs:string"/>
			<xs:element name="referenceNumber" type="referenceNumberType"/>
			<xs:element name="returnURL" type="xs:string" minOccurs="0"/>
			<xs:element name="asyncErrorURL" type="xs:string" minOccurs="0"/>
			<xs:element name="futureDateTime" type="oasDateType" minOccurs="0"/>
			<xs:element name="resend" type="xs:int" minOccurs="0"/>
			<xs:element name="userID" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="securityType">
		<xs:sequence>
			<xs:element name="userId" type="xs:string"/>
			<xs:element name="password" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ***TRANSACTION TYPES*** -->
	<xs:complexType name="resellerSubOrderBaseType">
		<xs:sequence>
			<xs:element name="subReferenceNumber" type="subReferenceNumberType" minOccurs="0"/>
			<xs:choice>
				<xs:element name="systemData" type="systemDataType" minOccurs="0"/>
				<xs:element name="serviceZipCode" type="zipCodeType" minOccurs="0"/>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="nextAvailableMDNType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
				Used by resellerAddSubscriberType.
				If an MDN is not supplied then the nextAvailableMDN must be supplied.
				If the npaNxx is not in the XML then the new MDN will be obtained
				using the oldMdn and the supplied zipCode.
				If the npaNxx and
				zipCode are not supplied then the new MDN will be
				obtained using the supplied state city and state.
				If the MDN is not supplied and none of these fields are supplied, then
				an exception will be
				returned to the reseller.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="npaNxx" type="npaNxxType" minOccurs="0"/>
			<xs:element name="zipCode" type="zipCodeType" minOccurs="0"/>
			<xs:element name="city" type="cityType" minOccurs="0"/>
			<xs:element name="state" type="stateType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="nextAvailablewithOldMDNType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
				Used by resellerChangeNumberType.
				If an MDN is not supplied then the nextAvailableOldMDN must be
				supplied.
				In nextAvailableMDN is supplied then the oldMdn must be supplied.
				The new MDN will be obtained using the oldMdn and the supplied npaNxx.
				If the npaNxx is not in the XML then the new MDN will be obtained
				using the oldMdn and the supplied zipCode.
				If the npaNxx and
				zipCode are not supplied then the new MDN will be
				obtained using the oldMdn and the supplied state city and state.
				If the MDN is not supplied and none of these fields are supplied, then
				an exception
				will be returned to the reseller.
			</xs:documentation>
