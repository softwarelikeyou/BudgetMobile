   <xs:element name="street2" type="xs:string" minOccurs="0" />
   <xs:element name="city" type="xs:string" minOccurs="1" />
   <xs:element name="state" type="stateType" minOccurs="1" />
   <xs:element name="zipCode" type="zipCodeType" minOccurs="1" />
   <xs:element name="country" type="countryType" minOccurs="0" />
  </xs:sequence>
 </xs:complexType>
 
 <xs:simpleType name="stateType">
  <xs:restriction base="xs:string">
   <xs:pattern value="[A-Z]{2}" />
  </xs:restriction>
 </xs:simpleType>

 <xs:simpleType name="countryType">
  <xs:restriction base="xs:string">
   <xs:maxLength value="3" />
  </xs:restriction>
 </xs:simpleType>

 <xs:simpleType name="zipCodeType">
  <xs:restriction base="xs:string">
   <xs:pattern value="[1-9][0-9][0-9][0-9][0-9]|[0-9][1-9][0-9][0-9][0-9]|[0-9][0-9][1-9][0-9][0-9]|[0-9][0-9][0-9][1-9][0-9]|[0-9][0-9][0-9][0-9][1-9]" />
  </xs:restriction>
 </xs:simpleType>

 <xs:simpleType name="phoneType">
