	       xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	       xmlns:tns="http://www.budgetprepay.com/MVNO/Verizon"
	       targetNamespace="http://www.budgetprepay.com/MVNO/Verizon"
	       elementFormDefault="qualified" 
	       attributeFormDefault="unqualified">
 <!-- Requests -->	       
 <xs:element name="DeviceInquiryRequest" 
             type="tns:DeviceInquiryRequest">
  <xs:annotation>
   <xs:documentation>
    To obtain the current status and details about the device. 
    Result is reuturned via Postback DeviceAsyncResponse type.
    The MIN element is optional.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="OrderInquiryRequest" 
             type="tns:OrderInquiryRequest">
  <xs:annotation>
   <xs:documentation>
    To inquire on the status of a request transaction sent via the XMLs that matches the Reference Number 
    provided during request. Note: Response will be sent via the OrderInquiryResponse which will be 
    transmitted synchronously.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="LineInquiryRequest" 
             type="tns:LineInquiryRequest">
  <xs:annotation>
   <xs:documentation>
    To get the current status and details about subscriber.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="PortInInquiryRequest" 
             type="tns:PortInInquiryRequest">
  <xs:annotation>
   <xs:documentation>
    To get the current details about a Porting request for the given subscriber.
   </xs:documentation>
  </xs:annotation>
 </xs:element> 
 <xs:element name="PrlInquiryRequest" 
             type="tns:PrlInquiryRequest">
  <xs:annotation>
   <xs:documentation>
    To get the current details about PRL Download for the given subscriber.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="DeviceActivationRequest" 
             type="tns:DeviceActivationRequest">
  <xs:annotation>
   <xs:documentation>
    To Activate an MDN(Mobile Directory Number) for an existing Reseller account. 
    The MDN or the nextAvailableMDN element is required. If the MDN is supplied, then the nextAvailableMDN 
    element may not be supplied. If the nextAvailableMDN element is supplied, then either the npaNxx or 
    the zipCode or both the city and state must be supplied.   
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="DeviceActivationPortInRequest" 
             type="tns:DeviceActivationPortInRequest">
  <xs:annotation>
   <xs:documentation>To activate a number via a Port In</xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="DeleteSubscriberRequest" 
             type="tns:DeleteSubscriberRequest">
  <xs:annotation>
   <xs:documentation>Publish Delete Subscribe Request</xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="ValidateMDNPortabilityRequest" 
             type="tns:ValidateMDNPortabilityRequest">
  <xs:annotation>
   <xs:documentation>To deactivate an active MDN on an existing Reseller account.</xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="ChangeESNRequest" 
             type="tns:ChangeESNRequest">
  <xs:annotation>
   <xs:documentation>
    To change the device(ESN/MEID) of an active 3G MDN of an existing Reseller account.
    </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="AddHotlineRequest" 
             type="tns:AddHotlineRequest">
  <xs:annotation>
   <xs:documentation>
    To hotline an active MDN of an existing reseller account to 
    the phone number provided within the transaction.
   </xs:documentation>
  </xs:annotation>
 </xs:element> 
 <xs:element name="RemoveHotlineRequest" 
             type="tns:RemoveHotlineRequest">
  <xs:annotation>
   <xs:documentation>
    To remove the hotline from an active MDN on an existing reseller account.
   </xs:documentation>
  </xs:annotation>
 </xs:element> 
 <xs:element name="AddHotlineSubscriberRequest" 
             type="tns:AddHotlineSubscriberRequest">
  <xs:annotation>
   <xs:documentation>
    To Activate an MDN in Hotline status on an existing Reseller account. 
    The SubOrgID tag is a conditional element and is used with MPNPoolName, 
    when more than one MPN (Mobile Private Network) is in use by a Reseller.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="ReconnectMDNRequest" 
             type="tns:ReconnectMDNRequest">
  <xs:annotation>
   <xs:documentation>
    To reconnect the deactivated MDN of an existing Reseller account within 
    the threshold period. Port-out MDN’s are not supported for reconnect.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="RestoreSuspendedSubscriberRequest" 
             type="tns:RestoreSuspendedSubscriberRequest">
  <xs:annotation>
   <xs:documentation>
    To restore the service of a previously suspended MDN on an existing reseller account.
   </xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="SuspendSubscriberRequest" 
             type="tns:SuspendSubscriberRequest">
  <xs:annotation>
   <xs:documentation>To suspend the active MDN on an existing reseller account.</xs:documentation>
  </xs:annotation>
 </xs:element>
 <xs:element name="SearchBucketRequest" 
             type="tns:SearchBucketRequest">
  <xs:annotation>
   <xs:documentation>Publish Search Bucket Request</xs:documentation>
  </xs:annotation>
 </xs:element> 
  <xs:element name="AddBucketRequest" 
             type="tns:AddBucketRequest">
  <xs:annotation>
