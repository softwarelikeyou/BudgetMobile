 <xs:complexType name="messageHeaderType">
  <xs:sequence>
   <xs:element name="userId" minOccurs="1">
    <xs:annotation><xs:documentation>A valid user within the Budget MVNO API System.</xs:documentation></xs:annotation>
    <xs:simpleType>
     <xs:restriction base="xs:string">
      <xs:maxLength value = "25"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
   <xs:element name="referenceNumber" minOccurs="1">
    <xs:annotation><xs:documentation>A unique indentifier.</xs:documentation></xs:annotation>
    <xs:simpleType>
     <xs:restriction base="xs:string">
      <xs:maxLength value = "30"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
   <xs:element name="planCode" minOccurs="1">
    <xs:annotation><xs:documentation>The appropriate VZW price plan code.</xs:documentation></xs:annotation>
    <xs:simpleType>
     <xs:restriction base="xs:string">
      <xs:maxLength value = "60"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
   <xs:element name="ansynchronousReturnUrl" minOccurs="0">
    <xs:annotation><xs:documentation>The URL for returning asynchronous POST responses, not required for synchronous operations</xs:documentation></xs:annotation>
    <xs:simpleType> 
     <xs:restriction base="xs:string">
      <xs:maxLength value = "255"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
  </xs:sequence>
 </xs:complexType>
 
 <xs:simpleType name="pinType">
  <xs:restriction base="xs:string">
   <xs:maxLength value="15" />
  </xs:restriction>
 </xs:simpleType>

 <xs:simpleType name="ospAccountNoType">
