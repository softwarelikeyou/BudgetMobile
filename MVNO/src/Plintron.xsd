 <xs:complexType name="SendMessageRequest">
  <xs:sequence>
   <xs:element name="messageHeader" type="messageHeaderType" minOccurs="1">
    <xs:annotation>
     <xs:documentation>Valid MVNO username</xs:documentation>
    </xs:annotation>
   </xs:element>  
   <xs:element name="MDN" type="xs:string" minOccurs="1">
    <xs:annotation>
     <xs:documentation>Destination MSISDN</xs:documentation>
    </xs:annotation>
   </xs:element>
   <xs:element name="dataCoding" type="xs:string" minOccurs="0" default="1">
    <xs:annotation>
     <xs:documentation>For 0,1 – Conversion from ASCII to GSM character set will be done</xs:documentation>
    </xs:annotation>
   </xs:element>
   <xs:element name="validityPeriod" type="xs:string" minOccurs="0">
    <xs:annotation>
     <xs:documentation>As per SMPP v3.4 specification “YYMMDDhhmmsstnnp”</xs:documentation>
    </xs:annotation>
   </xs:element>
   <xs:element name="scheduledDelivery" type="xs:string" minOccurs="0">
    <xs:annotation>
     <xs:documentation>As per SMPP v3.4 specification</xs:documentation>
    </xs:annotation>
   </xs:element> 
   <xs:element name="serviceType" type="xs:string" minOccurs="0" default="RN"></xs:element>     
   <xs:element name="senderTON" type="xs:string" minOccurs="0"></xs:element> 
   <xs:element name="sender" type="xs:string" minOccurs="0" default="7025">
    <xs:annotation>
     <xs:documentation>Should be 7025</xs:documentation>
    </xs:annotation>   
   </xs:element>    
   <xs:element name="message" type="xs:string" minOccurs="1"></xs:element>            
  </xs:sequence>
 </xs:complexType>
  	        
 <xs:complexType name="SendMessageResponse">
  <xs:sequence>
   <xs:element name="code" type="xs:string" minOccurs="1">
    <xs:annotation>
     <xs:documentation>0- Success, request is accepted by ESME. 1- Failure</xs:documentation>
    </xs:annotation>
   </xs:element>
   <xs:element name="error" type="xs:string" minOccurs="0"></xs:element>     
   <xs:element name="transactionId" type="xs:string" minOccurs="1"></xs:element>  
  </xs:sequence>
 </xs:complexType>
 
